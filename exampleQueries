mutation {
  createBreadCrumb(element: "div", type: "click") {
		id
  }
}


{
  breadCrumbs {
    id
    element
    type
    value
  }
}

start mongodb
# `mongod --config /usr/local/etc/mongod.conf`

mongo cli
# run `mongo "mongodb://localhost:27017"`  in terminal

# get all
db.breadCrumb.find()

# delete all
db.breadCrumb.deleteMany({})